{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "0.1",
  "browser_action": {
   "default_icon": "img/icon.png"
  },
  "permissions": [
    "activeTab"
    ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": ["lib/jquery.js", "src/content.js"]
    }
  ],
  "background": {
    "script": ["src/background.js"]
  }
}